                            {% if page.isActive %}
                                <li><a href="{{ path('pages_page', {'uri': page.uri}) }}">{{ page.name }}</a></li>
                            {% endif %}
                        {% endfor %}
                        {% if loop.index == 1 %}
                            {% if not app.user %}
                                <li><a href="{{ path('users_public_list') }}">חברים באתר</a></li>
                            {% endif  %}
                        {% endif %}
                        {% if loop.index == 3 %}
                            <li><a href="{{ path('faq') }}">שאלות ותשובות</a></li>
                        {% endif %}
                    </ul>
                </div>
            {% endfor %}


